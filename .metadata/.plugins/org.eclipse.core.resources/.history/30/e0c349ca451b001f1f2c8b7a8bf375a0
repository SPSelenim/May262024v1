package pomHome;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class Home {
public WebDriver driver=null;

public String FName="fname";
public String LName="lname";

	public Home(WebDriver driver)
	{
		this.driver=driver;
	}
	
	public WebElement getFirstName(String FName)
	{
		WebElement fname1 = driver.findElement(By.id(FName));
		return fname1;
	}
	
	public void Senkeys(WebElement ScreenElement,String Data)
	{
	ScreenElement.sendKeys("xxxx");
	}
	
	
	
}
